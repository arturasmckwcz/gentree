{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Person",
  "required": ["first", "last"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The id in database"
    },
    "first": {
      "$id": "#/properties/first",
      "type": "string",
      "title": "The first name"
    },
    "last": {
      "$id": "#/properties/last",
      "type": "string",
      "title": "The last name"
    },
    "birth": {
      "$id": "#/properties/birth",
      "type": "string",
      "title": "The birth date"
    },
    "gender": {
      "$id": "#/properties/gender",
      "type": "string",
      "title": "The gender"
    },
    "place_id": {
      "$id": "#/properties/place_id",
      "type": "string",
      "title": "The id of the place of birth"
    },
    "created_at": {
      "$id": "#/properties/created_at",
      "type": "string",
      "title": "The time record created_at"
    },
    "updated_at": {
      "$id": "#/properties/updated_at",
      "type": "string",
      "title": "The time record updated_at"
    }
  },
  "additionalProperties": false
}
