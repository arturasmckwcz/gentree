{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Place",
  "required": [
      "country"
  ],
  "properties": {
      "id": {
          "$id": "#/properties/id",
          "type": "integer",
          "title": "The id in database"
      },
      "country": {
          "$id": "#/properties/country",
          "type": "string",
          "title": "The country"
      },
      "place": {
        "$id": "#/properties/place",
        "type": "string",
        "title": "The place"
    },
      "created_at": {
          "$id": "#/properties/created_at",
          "type": "string",
          "title": "The time record created_at"
      },
      "updated_at": {
          "$id": "#/properties/updated_at",
          "type": "string",
          "title": "The time record updated_at"
      }
  },
  "additionalProperties": false
}